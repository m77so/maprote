{
  "compilerOptions": {
  "target": "ESNext",
  "useDefineForClassFields": true,
  "module": "CommonJS",
  "lib": ["ESNext"],
  "moduleResolution": "Node",
  "strict": true,
  "resolveJsonModule": true,
  "esModuleInterop": true,
  "noUnusedLocals": true,
  "noUnusedParameters": true,
  "noImplicitReturns": true,
  "skipLibCheck": true,
  "outDir": "dist"
  },
  "include": ["src"]
  }